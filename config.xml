<?xml version="1.0" encoding="UTF-8"?>

<!-- config.xml reference: https://build.phonegap.com/docs/config-xml -->

<widget xmlns = "http://www.w3.org/ns/widgets"

xmlns:gap = "http://phonegap.com/ns/1.0"

id = "com.fetalecho.PrematureLabor"

version = "1.0.0">

<name>Brown Plus Mobile</name>

<description>

This program describes the treatment of premature labor

</description>

<author href="http://www.fetal.com" email="grdevore@gmail.com">

Greggory R. DeVore, M.D.

</author>

<!-- Customize your app and platform with the preference element. -->

<preference name="phonegap-version" value="2.7.0" />

<preference name="orientation" value="default" />

<preference name="target-device" value="universal" />



<!-- Define app icon MAIN ROOT Platform. -->

<icon src="icon.png" />

<!-- Define app icon ANDROID Platform. -->

<icon src="img/icons/ldpi.png" gap:platform="android" gap:density="ldpi" />

<icon src="img/icons/mdpi.png" gap:platform="android" gap:density="mdpi" />

<icon src="img/icons/hdpi.png" gap:platform="android" gap:density="hdpi" />

<icon src="img/icons/xhdpi.png" gap:platform="android" gap:density="xhdpi" />

<icon src="img/icons/xxhdpi.png" gap:platform="android" gap:density="xxhdpi" />

<!-- Define app icon IOS Platform. -->

<icon src="icons/ios/icon.png" gap:platform="ios" width="57" height="57" />

<icon src="icons/ios/icon-72.png" gap:platform="ios" width="72" height="72" />

<icon src="icons/ios/icon_at_2x.png" gap:platform="ios" width="114" height="114" />

<icon src="icons/ios/icon-72_at_2x.png" gap:platform="ios" width="144" height="144" />

<!-- Define app splash screen for ANDROID platform. -->

<gap:splash src="img/splash/ldpi.png" gap:platform="android" gap:density="ldpi" />

<gap:splash src="img/splash/mdpi.png" gap:platform="android" gap:density="mdpi" />

<gap:splash src="img/splash/hdpi.png" gap:platform="android" gap:density="hdpi" />

<gap:splash src="img/splash/xhdpi.png" gap:platform="android" gap:density="xhdpi" />

<!-- Define app splash screen for IOS platform. -->

<gap:splash src="splash/ios/Default.png" width="320" height="480" />

<gap:splash src="splash/ios/Default_at_2x.png" width="640" height="960" />

<gap:splash src="splash/ios/Default_iphone5.png" width="640" height="1136" />

<gap:splash src="splash/ios/Default-Landscape.png" width="1024" height="768" />

<gap:splash src="splash/ios/Default-Portrait.png" width="768" height="1024" />

<gap:splash src="splash/ios/Default-Landscape_at_2x.png" width="2048" height="1496" />

<gap:splash src="splash/ios/Default-Portrait_at_2x.png" width="1536" height="2008" />

<accesss origin="*" />

</widget>